#!/bin/python3
FloatA=float(input("Enter a float value here: "))
FloatB=float(input("Enter a float value here: "))

print(FloatA,"+",FloatB,"=",FloatA+FloatB)
print(FloatA,"-",FloatB,"=",FloatA-FloatB)
print(FloatA,"*",FloatB,"=",FloatA*FloatB)
print(FloatA,"/",FloatB,"=",FloatA/FloatB)

print("\nThat's all, folks!")
